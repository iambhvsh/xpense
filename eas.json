{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "18.18.0",
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "18.18.0",
      "env": {
        "NODE_ENV": "production"
      },
      "android": {
        "gradleCommand": ":app:assembleRelease",
        "buildType": "apk",
        "withoutCredentials": true
      }
    },
    "production": {
      "node": "18.18.0",
      "android": {
        "gradleCommand": ":app:assembleRelease",
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
